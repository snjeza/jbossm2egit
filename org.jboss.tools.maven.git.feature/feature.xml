<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.jboss.tools.maven.git.feature"
      label="%featureName"
      version="1.0.0.qualifier"
      provider-name="%providerName"
      plugin="org.jboss.tools.maven.git">

   <description>
      %description
   </description>

   <copyright>
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <requires>
      <import feature="org.eclipse.egit" version="0.11.0" match="greaterOrEqual"/>
      <import feature="org.eclipse.jgit" version="0.11.0" match="greaterOrEqual"/>
      <import feature="org.jboss.tools.maven.cdi.feature" version="1.0.0" match="greaterOrEqual"/>
      <import feature="org.jboss.tools.maven.feature" version="1.1.0" match="greaterOrEqual"/>
      <import feature="org.jboss.tools.maven.hibernate.feature" version="1.0.0" match="greaterOrEqual"/>
      <import feature="org.jboss.tools.maven.jsf.feature" version="1.0.0" match="greaterOrEqual"/>
      <import feature="org.jboss.tools.maven.portlet.feature" version="1.0.0" match="greaterOrEqual"/>
      <import feature="org.jboss.tools.maven.seam.feature" version="1.1.0" match="greaterOrEqual"/>
      <import feature="org.maven.ide.eclipse.feature" version="0.12.0" match="greaterOrEqual"/>
      <import feature="org.maven.ide.eclipse.wtp.feature" version="0.11.1" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.jboss.tools.maven.git"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
